<%= form_for [:admin, @league, @season, @game] do |f| %>
	<div class="form-control">
		<span class="form_label">
			<%= f.label :league_id %>
		</span>
		<span class="form_input">
			  <%= f.number_field :league_id %>
		</span>
	</div>
	<div class="form-control">
		<span class="form_label">
			<%= f.label :season_id %>
		</span>
		<span class="form_input">
			<%= f.number_field :season_id %>
		</span>
	</div>
	<div class="form-control">
		<%= f.label :game %>
		<%= f.number_field(:game)%>
	</div>
	<div class="form-control input-group date" id='datetimepicker2'>
		<%= f.label :data %>
		<%= f.text_field(:data) %>
		<span class="input-group-addon">
			<span class="glyphicon glyphicon-calendar"></span>
		</span>
	</div>
	<div class="form-control">
		<%= f.label :location %>
		<%= f.text_field(:location)%>
	</div>
	<div class="form-control">
		<%= f.label :adress %>
		<%= f.text_field(:adress)%>
	</div>
		<%= f.submit 'Сохранить', :class => "btn btn-primary m-t-2" %>
<% end %>
